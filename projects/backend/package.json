{
	"name": "@lab77store/backend",
	"version": "0.8.3",
	"private": true,
	"license": "UNLICENSED",
	"main": "dist/index.js",
	"engines": {
		"node": "14.x"
	},
	"scripts": {
		"clean": "rm -rf node_modules/ dist/",
		"prebuild": "rimraf dist",
		"heroku-prebuild": "node ./pre-build",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "npm run start:prod",
		"start:dev": "env NODE_ENV=development nest start --watch",
		"start:debug": "env NODE_ENV=development nest start --debug --watch",
		"start:emulator": "firebase emulators:start --only functions",
		"start:prod": "node dist/main",
		"start:firebase": "yarn shell",
		"lint": "yarn lint:cmd src",
		"lint:cmd": "eslint --ext ts --ext js",
		"lint:fix": "eslint --fix --ext ts --ext js",
		"test": "env NODE_ENV=test jest --maxWorkers=1",
		"test:watch": "env NODE_ENV=test jest --watch",
		"test:cov": "env NODE_ENV=test jest --coverage",
		"test:debug": "env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:all": "yarn lint && yarn test",
		"serve": "yarn build && firebase serve --only functions",
		"shell": "yarn build && firebase functions:shell",
		"deploy": "firebase deploy --only functions",
		"logs": "firebase functions:log",
		"pre-commit": "lint-staged",
		"pre-push": "yarn lint && yarn test",
		"bkp:mongodb": "cd .docker/mongodb && rm -rf ./seed && mongodump --uri=mongodb://lab77:lab77@localhost/dev-lab77 --out=seed",
		"bkp": "yarn bkp:mongodb"
	},
	"lint-staged": {
		"*.@(ts|tsx)": [
			"yarn lint",
			"yarn format"
		]
	},
	"dependencies": {
		"@lab77store/database": "^0.8.3",
		"@lab77store/domain": "^0.8.3",
		"@nest-modules/mailer": "^1.3.22",
		"@nestjs-modules/mailer": "^1.5.1",
		"@nestjs/common": "^7.6.15",
		"@nestjs/config": "^0.6.3",
		"@nestjs/core": "^7.6.15",
		"@nestjs/mongoose": "^7.1.2",
		"@nestjs/passport": "^7.1.0",
		"@nestjs/platform-express": "^7.6.15",
		"@nestjs/sequelize": "^0.1.1",
		"@nestjs/swagger": "^4.7.5",
		"@shelf/winston-datadog-logs-transport": "^2.0.1",
		"applicationinsights": "^2.1.9",
		"await-to-js": "^2.1.1",
		"aws-sdk": "^2.696.0",
		"axios": "^0.21.1",
		"cache-manager": "^3.6.0",
		"class-transformer": "^0.3.1",
		"class-validator": "^0.12.2",
		"cloudinary": "^1.22.0",
		"connect-datadog": "^0.0.9",
		"cpf-cnpj-validator": "^1.0.3",
		"dayjs": "^1.10.3",
		"express-http-context": "^1.2.4",
		"express-rate-limit": "^5.1.3",
		"express-status-monitor": "^1.3.3",
		"express-winston": "^4.2.0",
		"firebase-admin": "8.12.1",
		"firebase-functions": "^3.11.0",
		"handlebars": "^4.7.7",
		"helmet": "^3.23.0",
		"lodash": "^4.17.21",
		"module-alias": "^2.2.2",
		"mongoose": "^5.11.4",
		"mongoose-id-validator": "^0.6.0",
		"mysql2": "^2.2.5",
		"nest-winston": "^1.6.1",
		"nestjs-mailer": "^1.0.1",
		"nodemailer": "^6.4.8",
		"number-precision": "^1.5.0",
		"passport": "^0.4.1",
		"passport-http-bearer": "^1.0.1",
		"passport-local": "^1.0.0",
		"query-string": "^6.13.1",
		"randomstring": "^1.1.5",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^6.6.6",
		"sequelize": "5.21.10",
		"sequelize-typescript": "^1.1.0",
		"swagger-ui-express": "^4.1.5",
		"throng": "^5.0.0",
		"urlify": "^0.3.6",
		"uuid": "^8.3.2",
		"valid-url": "^1.0.9",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@nestjs/cli": "^7.6.0",
		"@nestjs/schematics": "^7.3.0",
		"@nestjs/testing": "^7.6.15",
		"@types/busboy": "^0.2.3",
		"@types/cache-manager": "^3.4.2",
		"@types/compression": "^1.7.0",
		"@types/express": "^4.17.11",
		"@types/express-rate-limit": "^5.0.0",
		"@types/handlebars": "^4.1.0",
		"@types/helmet": "0.0.47",
		"@types/jest": "^26.0.23",
		"@types/lodash": "^4.14.171",
		"@types/mongoose-id-validator": "^0.6.0",
		"@types/node": "^14.14.36",
		"@types/nodemailer": "^6.4.0",
		"@types/passport-http-bearer": "^1.0.35",
		"@types/passport-local": "^1.0.33",
		"@types/randomstring": "^1.1.6",
		"@types/sequelize": "^4.28.9",
		"@types/supertest": "^2.0.10",
		"@types/throng": "^5.0.3",
		"@types/uuid": "^8.0.0",
		"@types/valid-url": "^1.0.3",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-prettier": "^3.3.1",
		"firebase": "^8.2.4",
		"jest": "26.6.0",
		"lint-staged": "^10.5.3",
		"nock": "^13.0.11",
		"prettier": "2.2.1",
		"supertest": "^6.1.3",
		"ts-jest": "^26.5.4",
		"ts-loader": "^8.0.18",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.3.2"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"coverageReporters": [
			"json",
			"html",
			"lcov",
			"text",
			"clover"
		],
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"<rootDir>/server.ts",
			"<rootDir>/Modules/Importation/"
		],
		"moduleNameMapper": {
			"@App(.*)$": "<rootDir>/$1",
			"@Utils(.*)$": "<rootDir>/Utils$1",
			"@Modules(.*)$": "<rootDir>/Modules$1"
		}
	},
	"_moduleAliases": {
		"@App": "./dist",
		"@Utils": "./dist/Utils",
		"@Modules": "./dist/Modules"
	}
}
